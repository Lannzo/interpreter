<block_item> ::= <statement> 
<statement> ::= <input_statement>
<input_statement> ::= "scanf" "(" <string> { "," "&" <identifier> } ")" ";"
		       ::= "scanf" "(" <string> { "," "&" <identifier> } "," "&" <identifier> ")" ";"
		       ::= "scanf" "(" <string> { "," "&" <identifier> } "," "&" value2 ")" ";"
		       ::= "scanf" "(" <string> "," "&" <identifier> "," "&" value2 ")" ";"
		       ::= "scanf" "(" "%f %f" "," "&" value1 "," "&" value2 ")" ";"